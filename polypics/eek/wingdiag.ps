% Draw the diagram of how the wings go on to Dodecahedral Eek!.

% Import the definition of `wing' from wing.ps, having first
% defined DONOTHING to inhibit it actually doing a showpage.
/DONOTHING true def
(wing.ps)(r)file cvx exec

/pentagon {
  % Draw a pentagon with side length 1.
  % Do this LOGO-style, constantly moving our current matrix in a
  % mad fashion.
  matrix currentmatrix % save our original matrix
  currentpoint translate
  0 0 moveto
  4 { 0 1 translate 0 0 lineto 72 rotate } repeat
  setmatrix
  closepath
} def

30 dup scale

gsave
  2 1.0 translate
  gsave
    matrix currentmatrix
    -0.1 0 translate
    [ 0.8 0.2 0 0.8 0 0 ] concat
    0 0 moveto pentagon
    setmatrix
    gsave 0.9 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    +0.1 0 translate
    [ -0.8 0.2 0 0.8 0 0 ] concat
    0 0 moveto pentagon
    setmatrix
    gsave 0.7 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    -0.1 0 translate
    [ -0.8 0.2 0 0.8 0 0 ] concat
    0.2 dup scale -5 -10 translate wingtab
    setmatrix
    gsave 0.7 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
grestore

newpath 4.0 1.4 moveto 5.4 1.4 lineto closepath 0.1 setlinewidth 0 setgray stroke
newpath 6.0 1.4 moveto 5.3 1.6 lineto 5.3 1.2 lineto closepath 0 setgray fill

gsave
  8 1.0 translate
  gsave
    matrix currentmatrix
    -0.05 0.8 translate
    [ 0.2 0.5 0 -0.8 0 0 ] concat
    0.2 dup scale -5 -10 translate wingmain
    setmatrix
    gsave 1 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    +0.05 0.8 translate
    [ 0.2 0.5 0 -0.8 0 0 ] concat
    0.2 dup scale -5 -10 translate wingmain
    setmatrix
    gsave 0 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    -0.2 0 translate
    [ 0.8 0.2 0 0.8 0 0 ] concat
    0 0 moveto pentagon
    setmatrix
    gsave 0.9 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    +0.2 0 translate
    [ -0.8 0.2 0 0.8 0 0 ] concat
    0 0 moveto pentagon
    setmatrix
    gsave 0.7 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    +0.05 0 translate
    [ -0.8 0.2 0 0.8 0 0 ] concat
    0.2 dup scale -5 -10 translate wingtab
    setmatrix
    gsave 0.7 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
  gsave
    matrix currentmatrix
    -0.05 0 translate
    [ 0.8 0.2 0 0.8 0 0 ] concat
    0.2 dup scale -5 -10 translate wingtab
    setmatrix
    gsave 0.9 setgray fill grestore 0.01 setlinewidth 0 setgray stroke
  grestore
grestore

showpage

