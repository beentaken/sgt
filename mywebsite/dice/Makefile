	gs -sDEVICE=bmp16 -sOutputFile=tmp.bmp -g300x300 -r216 \
		-dBATCH -dNOPAUSE -q - $<
	convert -geometry 30x30 -antialias tmp.bmp $@
	rm -f tmp.bmp

