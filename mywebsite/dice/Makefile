face2a.png: face.ps
	echo '(2) 1 ()' | \
	gs -sDEVICE=bmp16 -sOutputFile=tmp.bmp -g300x300 -r216 \
		-dBATCH -dNOPAUSE -q - $<
	convert -geometry 75x75 -antialias tmp.bmp $@
	rm -f tmp.bmp

